<!--pages/order-list/index.wxml-->
<!-- 顶部选择 -->
<view class="top_list">
  <view data-index="{{index}}" bindtap='onTopClick' class="{{topListIndex==index?'active':''}}" wx:for="{{topList}}" wx:key="{{index}}">
    {{item}}
  </view>
</view>
<view class="zhanwei"></view>

<!-- 订单列表 -->
<view class="order_list" bindtap='onOrderDetail'>
  <!-- 商品 -->
  <view class="goodsItem">
    <image class="goodsItemImg" src="https://tyigou.haoshichengduo.com/index_url_0.png"></image>
    <view class="goodsItemRight">
      <view class="goodsItemRightName">晨光中性替芯 全针管中心笔替芯12支HAGR0121黑0.35</view>
      <view class="goodsItemRightGuiGe">规格：0.5mm黑色替芯桶装12支</view>
      <view class="goodsItemRightNum">
        <view class="goodsItemRightNumMoney">¥7.5</view>
        <view class="goodsItemRightNumShuLiang">x2</view>
      </view>
    </view>
  </view>
  <!-- 合计 -->
  <view class="heji">合计：￥8.0（含邮费）</view>
  <!-- 按钮 -->
  <view class="anniu">
    <text>取消订单</text>
    <text class="t1">付款</text>
  </view>
</view>

