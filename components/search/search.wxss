/* components/search/search.wxss */
._search_wrapper {
    width: 100%;
    display: flex;
    flex-direction: row;
    align-items: center;
    box-sizing: border-box;
    height:66rpx;
    background:rgba(246,246,246,1);
    border-radius:33rpx;
    padding-left: 33rpx;
    font-size:30rpx;
    font-weight:500;
    color:rgba(194,194,194,1);
}
._search_wrapper image {
    margin-right: 20rpx;
    width: 32rpx;
    height: 32rpx;
}