/* components/add/add.wxss */
._add{
  width:148rpx;
height:44rpx;
background:rgba(255,255,255,1);
border:1rpx solid rgba(194,194,194,1);
border-radius:3rpx;
display: flex;
align-items: center;
}
._add_left{
  display: flex;
  align-items: center;
  justify-content: center;
  width:50rpx;
  height: 44rpx;
  border-right: 1rpx solid rgba(194,194,194,1);
  box-sizing: border-box;
}
._add_left image{
  width: 20rpx;
  height: 20rpx;
}

._add_num{
  height: 44rpx;
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:24rpx;
/* font-family:PingFang-SC-Medium; */
font-weight:500;
color:rgba(51,51,51,1);
}

._add_right{
  display: flex;
  align-items: center;
  justify-content: center;
  width:50rpx;
  height: 44rpx;
  border-left: 1rpx solid rgba(194,194,194,1);
  box-sizing: border-box;
}
._add_right image{
  width: 20rpx;
  height: 20rpx;
}